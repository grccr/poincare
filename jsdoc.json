{
  "source": {
    "include": [ "package.json", "readme.md", "src/poincare/index.js", "src" ],
    "exclude": [ "node_modules", "docs" ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false
  },
  "opts": {
    "destination": "./docs/",
    "private": true,
    "recurse": true,
    "encoding": "utf8",
    "template": "./node_modules/minami"
  }
}